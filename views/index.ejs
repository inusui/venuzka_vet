<section id="all">
    <div class="gallery_section_2">
    	<div class="container">
    		<div class="row">
    			<div class="col-sm-8">
    				<<table class="tarjeta">
						<h2 class="pet_taital">Todas las tarjetas</h2>
					<br><br><br><br><br>
						<% veterinaria.forEach(function(veterinaria){%>
						<tr><th>Datos Generales de  <h3><%= veterinaria.value.PetName %></h3> </th></tr>
						<tr>
							<th>Identificacion: <%= veterinaria.id %> </th>
							<th>Raza: <%= veterinaria.value.Raza %> </th>
							<th>Sexo <%= veterinaria.value.Sexo %></th>
							<th>Fecha de Nacimiento: <%= veterinaria.value.Birth %></th>
						</tr>
						<tr>
							<td>Nombre del Dueño: <%= veterinaria.value.PetOwnerName %></td>
							<td>Telefono: <%= veterinaria.value.OwnerPhone %></td>
							<td>
								<form method="post" action="/veterinaria/delete/<%= veterinaria.key %>">
									<input  type="hidden" value="<%= veterinaria.value.rev %>" name="rev">
									<input type="submit" class="botonAlingRight" onclick="return confirm('Seguro que desea Eliminar este Registro?');" value="Eliminar Registro X">
								</form>
							</td>
							<td>
								
							</td>
						</tr>
	
						<% }); %>
	
